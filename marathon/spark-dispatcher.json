{
    "id": "/spark-dispatcher",
    "cpus": 1,
    "mem": 2048.0,
    "requirePorts": false,
    "instances": 1,
    "cmd": "LIBPROCESS_IP=$HOST SPARK_LOCAL_IP=$HOST SPARK_HOME/sbin/start-mesos-dispatcher.sh --host $HOST --master mesos://master.mesos:5050",
    "constraints": [["hostname", "UNIQUE"]],
    "backoffSeconds": 1,
    "backoffFactor": 1.15,
    "maxLaunchDelaySeconds": 3600,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.2
    }
}
