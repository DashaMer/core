{
  "id": "/spark-dispatcher",
  "cpus": 0.5,
  "mem": 300,
  "requirePorts": false,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "pintostack/spark-dispatcher",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 7077,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "privileged": false
    }
  },
  "env":{
    "SERVICE_PORT0_NAME": "{{NAME}}-UI",
    "SERVICE_PORT1_NAME": "{{NAME}}"
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "gracePeriodSeconds": 60,
      "intervalSeconds": 5,
      "portIndex": 0,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 120
    }
  ],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.5,
    "maximumOverCapacity": 0.2
  }
}