{
    "id": "/ipythonnb",
    "cpus": 0.5,
    "mem": 768,
    "requirePorts": false,
    "instances": 1,
    "cmd": "./bootstrap.sh",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "pytnru/pintostack:ipythonnb",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 31080,
                    "hostPort": 31080,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                },
                 {
                    "containerPort": 0,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "privileged": false
        }
    },
    "env":{
        "SERVICE_PORT0_NAME": "{{NAME}}",
        "SERVICE_PORT1_NAME": "{{NAME}}-libprocess",
        "SERVICE_PORT2_NAME": "{{NAME}}-spark-driver",
        "SERVICE_PORT3_NAME": "{{NAME}}-spark-fileserver",
        "SERVICE_PORT4_NAME": "{{NAME}}-spark-broadcast",
        "SERVICE_PORT5_NAME": "{{NAME}}-spark-blockmanager",
        "SERVICE_PORT6_NAME": "{{NAME}}-spark-ui",
        "SERVICE_PORT7_NAME": "{{NAME}}-spark-replclassserver"
    },
    "constraints": [["hostname", "UNIQUE"]],
    "healthChecks": [
        {
            "protocol": "TCP",
            "gracePeriodSeconds": 300,
            "intervalSeconds": 5,
            "portIndex": 0,
            "timeoutSeconds": 60,
            "maxConsecutiveFailures": 5
        }
    ],
    "backoffSeconds": 1,
    "backoffFactor": 1.15,
    "maxLaunchDelaySeconds": 3600,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.2
    }
}
