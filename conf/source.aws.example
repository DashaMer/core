### Amazon AWS Account Parametrs
# For more information refere to https://github.com/pintostack/core

source conf/source.global

# All variables add below

export AWS_KEY_ID=''
export AWS_ACCESS_KEY=''
export AWS_KEYPAIR_NAME='pinto'
export AWS_AMI='ami-df6a8b9b'
export AWS_INSTANCE_TYPE='m3.large'
export AWS_ROOT_PARTITION_SIZE=50
export AWS_REGION='us-west-1'
export AWS_TERMINATE_ON_SHUTDOWN='true'
export AWS_SECURITY_GROUPS="default,default-ssh"
export AWS_SSH_USERNAME='ubuntu'
export SSH_KEY_FILE=/pintostack/conf/pinto.pem
